{
    "name": "ahinkle/packagist-latest-version",
    "description": "Obtains the latest version release of Composer Packages from the Packagist API",
    "keywords": [
        "ahinkle",
        "packagist-latest-version",
        "packagist",
        "packagist-api",
        "latest",
        "composer",
        "version"
    ],
    "homepage": "https://github.com/ahinkle/packagist-latest-version",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andy Hinkle",
            "email": "ahinkle10@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "spatie/packagist-api": "^1.2 | ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ahinkle\\PackagistLatestVersion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ahinkle\\PackagistLatestVersion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
